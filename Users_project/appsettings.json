{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionString": {
    "UserRegisterAPIDb": "Server=.;Database=UserRegisterAPIDb;Trusted_Connection=True;TrustServerCertificate=True;"

  },
  "Jwt": {
    "Key": "",
    "Issuer": "UserApi",
    "Audience": "UserRegisteredAPI"
  },
  
  "serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "file",
        "Args": {
          "path": "./logs/log-txt",
          "rollinInterval": "Day"
        }
      },
      {

        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  }
}
